{
    "manifest_version": 2,
    "name": "Website Environment Identifier",
    "version": "0.0.1",
    "description": "Prefix titles websites to identify environment",
    "homepage_url": "https://github.com/sylouuu/website-environment-identifier",

    "icons": {
        "16": "img/icon_16.png",
        "32": "img/icon_32.png",
        "48": "img/icon_48.png",
        "128": "img/icon_128.png"
    },

    "background": {
        "scripts": ["js/app/background.js"]
    },

    "options_page": "html/options.html",

    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}